<UserControl
    x:Class="TileEditorTest.View.Controls.NineGridPoints"

    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:TileEditorTest.View.Controls"

    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d" 
    PointerMoved="UserControl_PointerMoved"
    Tapped="UserControl_Tapped"
    RightTapped="UserControl_RightTapped"
    >

    <UserControl.Resources>
        <Color x:Key="stroke"  >#ff0000</Color>
        <Color x:Key="fill"  >#33ff0000</Color>
        <x:Double x:Key="strokeThickness">0.5</x:Double>
        <ScalarTransition x:Key="opacityTransition" Duration="0:0:0.2"></ScalarTransition>

    </UserControl.Resources>

    <Grid  x:Name="pathHolder">
        <Grid.Resources>
            <Style TargetType="ItemsControl" x:Key="PathControlStyle">
                <Setter Property="ItemsPanel">
                    <Setter.Value>
                        <ItemsPanelTemplate>
                            <Grid/>
                        </ItemsPanelTemplate>
                    </Setter.Value>
                </Setter>
                <Setter Property="ItemTemplate">
                    <Setter.Value>
                        <DataTemplate x:DataType="local:TerrainData">
                            <Path HorizontalAlignment="Stretch"
                          VerticalAlignment="Stretch"
                          Opacity="0.5"
                          StrokeThickness="{StaticResource strokeThickness}"  
                          Data="{x:Bind Path}"
                          >
                                <Path.Fill>
                                    <SolidColorBrush Color="{x:Bind  Model.Fill,Mode=OneWay}"  />
                                </Path.Fill>
                                <Path.Stroke>
                                    <SolidColorBrush Color="{x:Bind Model.Stroke,Mode=OneWay}"  />
                                </Path.Stroke>
                            </Path>

                        </DataTemplate>
                    </Setter.Value>
                </Setter>
            </Style>
        </Grid.Resources>


        <Rectangle  Fill="Magenta" />
        <Image UseLayoutRounding="False" HorizontalAlignment="Left" VerticalAlignment="Top" Grid.Row="2" Source="{x:Bind ViewModel.ImageSource, Mode=OneWay}"  Stretch="None"  />

        <ItemsControl ItemsSource="{x:Bind Paths, Mode=OneWay}" Style="{StaticResource PathControlStyle}" />
        <Canvas>

            <ItemsControl Canvas.Left="{x:Bind HoverTileX, Mode=OneWay}" Canvas.Top="{x:Bind HoverTileY, Mode=OneWay}" ItemsSource="{x:Bind MousePaths, Mode=OneWay}" Style="{StaticResource PathControlStyle}" />
        </Canvas>
        <local:AlignmentGrid TileWidth="{x:Bind ViewModel.TileWidth}" TileHeight="{x:Bind ViewModel.TileHeight}" />

